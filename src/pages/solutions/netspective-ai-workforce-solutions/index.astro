---
import NetspectiveLayout from "../../../layouts/ds/tailwind/NetspectiveLayout.astro";
import { routes, type Route } from "../../../lib/native/route";

export interface Props {
	readonly route: Route<any, any>;
}


const { route } = Astro.props;
const r = routes(Astro, Astro.props.route);
const allPosts = await Astro.glob("../../../content/solutions/netspective-ai-workforce-solutions/**/*.{md,mdx}");
allPosts.sort((a, b) => {
  const aHdoIlo = a.frontmatter.weight ?? 0;
  const bHdoIlo = b.frontmatter.weight ?? 0;
   return aHdoIlo < bHdoIlo ? -1 : (aHdoIlo > bHdoIlo ? 1 : 0);
});
---

<NetspectiveLayout route={route} {r}>
  <main>    
    <!-- inner page > starts -->
    <section class="px-8 sm:px-6 lg:px-8 bg-cover bg-no-repeat bg-center py-10" style="background-image: url(/assets-natural/brand/www.netspective.com/solutions/header-bg.jpg);">
      <article class="mx-auto max-w-4xl py-9 text-white text-center">
        <h3 class="text-2xl md:text-4xl font-semibold pb-5 uppercase">Netspective AI Workforce: Staff Augmentation with Intelligent Agents</h3>        
        <p class="text-xl pb-5 text-gray-100">The future of work is here, and it’s powered by <span class="font-bold">Artificial Intelligence (AI).</span> Netspective's <span class="font-bold">AI Workforce Solutions</span> enable businesses to augment their human workforce with AI agents, advanced automation, and intelligent orchestration.</p>
        <p class="text-xl pb-5 text-gray-100">With <span class="font-bold">agentic AI</span> and <span class="font-bold">large language models (LLMs),</span> Netspective’s contract engineering teams empower organizations to achieve unparalleled efficiency, precision, and scalability. From task automation to intelligent decision-making, our solutions are tailored to meet the demands of tomorrow’s workforce, today.</p>

         <div class="grid grid-cols-1 md:grid-cols-4 gap-4">
          <div class="my-2">
          <a href="/solutions/netspective-ai-workforce-solutions/#1-platform--software-solutions" class="text-white">
            <p class="mb-1 flex justify-center"><img src={r.uaAssetURL.brand(
              "/www.netspective.com/solutions/ai-workforce/ico-AI-workforce-1.png"
            )} alt="Patient-Content"></p>
            <p class="leading-5">Platform & Software Solutions</p>
          </a>
          </div>
          <div class="my-2">
            <a href="/solutions/netspective-ai-workforce-solutions/#2-ai-integration--consulting" class="text-white">
              <p class="mb-1 flex justify-center"><img src={r.uaAssetURL.brand(
                "/www.netspective.com/solutions/ai-workforce/ico-AI-workforce-2.png"
              )} alt="Information Button"></p>
              <p class="leading-5">AI Integration & Consulting</p>
            </a>  
          </div>
          <div class="my-2">
            <a href="/solutions/netspective-ai-workforce-solutions/#3-application-monitoring--management" class="text-white">
              <p class="mb-1 flex justify-center"><img src={r.uaAssetURL.brand(
                "/www.netspective.com/solutions/ai-workforce/ico-AI-workforce-3.png"
              )} alt="Plug-ins and Extensions"></p>
              <p class="leading-5">Application Monitoring & Management</p>
            </a>  
          </div>
          <div class="my-2">
            <a href="/solutions/netspective-ai-workforce-solutions/#4-downloadable-ai-software" class="text-white">
              <p class="mb-1 flex justify-center"><img src={r.uaAssetURL.brand(
                "/www.netspective.com/solutions/ai-workforce/ico-AI-workforce-4.png"
              )} alt="Loyalty-Rewards"></p>
              <p class="leading-5">Downloadable AI Software</p>
            </a>  
          </div>
          <div>
          </div>
        </div>

               
      </article>
    </section> 
    
    <!-- inner nav > starts -->
    <section>
      <nav class="drop-shadow-xl text-white bg-[radial-gradient(ellipse_at_top,_var(--tw-gradient-stops))] from-red-600 via-red-900 to-red-900">
        <div class="mx-auto max-w-7xl px-4 sm:px-6 lg:px-8">
          <div class="flex h-16 justify-between">
            <div class="flex">
              <div class="-ml-2 mr-2 flex items-center md:hidden">
                <!-- Mobile menu button -->
                <button type="button" data-collapse-toggle="contexxio-mobile-menu" class="inline-flex items-center justify-center rounded-md p-2 text-gray-400 hover:bg-gray-100 hover:text-gray-500 focus:outline-none focus:ring-2 focus:ring-inset focus:ring-indigo-500" aria-controls="contexxio-mobile-menu" aria-expanded="false">
                  <span class="sr-only">Open main menu</span>
                  <svg class="block h-6 w-6" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" aria-hidden="true">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25h16.5" />
                  </svg>
                  <svg class="hidden h-6 w-6" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" aria-hidden="true">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M6 18L18 6M6 6l12 12" />
                  </svg>
                </button>
              </div>
              <div class="flex flex-shrink-0 items-center uppercase text-xl">
                <a href="/solutions/netspective-ai-workforce-solutions/" class="text-white">Netspective AI Workflow Solutions</a>
              </div>
              <div class="hidden md:ml-6 md:flex md:space-x-8 text-base font-normal">
                <a href="/solutions/netspective-ai-workforce-solutions/#revolutionizing-work-with-ai" class="inline-flex items-center px-1 pt-1 text-gray-300">Revolutionizing Work with AI</a>
                <a href="/solutions/netspective-ai-workforce-solutions/#our-services" class="inline-flex items-center px-1 pt-1 text-white hover:text-gray-300">Our Services</a>
                <a href="/solutions/netspective-ai-workforce-solutions/#industries-we-serve" class="inline-flex items-center px-1 pt-1 text-white hover:text-gray-300">Industries We Serve</a>
                <a href="/solutions/netspective-ai-workforce-solutions/#why-choose-netspective" class="inline-flex items-center px-1 pt-1 text-white hover:text-gray-300">Why Choose Netspective</a>
              </div>
            </div>
          </div>
        </div>
      
        <!-- Mobile menu, show/hide based on menu state. -->
        <div class="md:hidden hidden" id="contexxio-mobile-menu">
          <div class="space-y-1 pb-3 pt-2">
            <a href="/solutions/netspective-ai-workforce-solutions/#revolutionizing-work-with-ai" class="block border-l-4 border-transparent py-2 pl-3 pr-4 text-white hover:border-red-400 hover:bg-gray-50 hover:text-gray-800">Revolutionizing Work with AI</a>
            <a href="/solutions/netspective-ai-workforce-solutions/#our-services" class="block border-l-4 border-transparent py-2 pl-3 pr-4 text-white hover:border-red-400 hover:bg-gray-50 hover:text-gray-800">Our Services</a>
            <a href="/solutions/netspective-ai-workforce-solutions/#industries-we-serve" class="block border-l-4 border-transparent py-2 pl-3 pr-4 text-white hover:border-red-400 hover:bg-gray-50 hover:text-gray-800">Industries We Serve</a>
            <a href="/solutions/netspective-ai-workforce-solutions/#why-choose-netspective" class="block border-l-4 border-transparent py-2 pl-3 pr-4 text-white hover:border-red-400 hover:bg-gray-50 hover:text-gray-800">Why Choose Netspective</a>
          </div>
        </div>
      </nav>
    </section>
    <!-- inner nav < ends -->
    
    <section class="mx-auto max-w-7xl px-4 py-4 sm:px-6 lg:px-8 mb-8"> 
      {
        allPosts.length > 0
          ? allPosts.map(
              (post) => ( 
      <div id={post.frontmatter.slugid} class="border border-x-0 border-t-0 border-gray-100">
        <article class="pt-10 pb-9">
          <div class="text-center">
            <h3 class="text-2xl font-bold mb-10 text-center inline-flex"><span class="bg-orange-600 text-white py-2 px-8">{post.frontmatter.title}
            </span></h3>
          </div>
          <div set:html={post.compiledContent()} class="pb-6 curated-content">
          </div>
        </article>
      </div>
      ),
            )
          : "No data"
      }
    </section>
    
    <section class="px-8 sm:px-6 lg:px-8 bg-cover bg-no-repeat bg-center py-10 bg-image">
        <article class="mx-auto max-w-6xl py-9 text-white text-center">
          <h3 class="text-2xl font-semibold pb-5">Netspective is a leader in Technology and Consulting services in regulated markets - Healthcare, Government and Medical Technology.</h3>
          <p>Technology, consulting, and solutions focused on firms impacted by FDA, ONC, NIST or other safety, privacy, and security regulations.</p>
        </article>
    </section>
    <!-- inner page < ends -->
  </main>
  <div class="clear-both"></div>
</NetspectiveLayout>
