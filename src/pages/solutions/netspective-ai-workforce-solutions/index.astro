---
import NetspectiveLayout from "../../../layouts/ds/tailwind/NetspectiveLayout.astro";
import { routes, type Route } from "../../../lib/native/route";
import { Content } from "../../../content/solutions/netspective-ai-workforce-solutions/netspective-ai-workforce-solutions.md";

export interface Props {
	readonly route: Route<any, any>;
}


const { route } = Astro.props;
const r = routes(Astro, Astro.props.route);
---

<NetspectiveLayout route={route} {r}>
  <main>    
    <!-- inner page > starts -->
    <section class="px-8 sm:px-6 lg:px-8 bg-cover bg-no-repeat bg-center py-10" style="background-image: url(/assets-natural/brand/www.netspective.com/solutions/header-bg.jpg);">
      <article class="mx-auto max-w-4xl py-9 text-white text-center">
        <h3 class="text-2xl md:text-4xl font-semibold pb-5 uppercase">Netspective AI Workforce: Staff Augmentation with Intelligent Agents</h3>        
        <p class="text-xl pb-5 text-gray-100">The future of work is here, and it’s powered by <span class="font-bold">Artificial Intelligence (AI).</span> Netspective's <span class="font-bold">AI Workforce Solutions</span> enable businesses to augment their human workforce with AI agents, advanced automation, and intelligent orchestration.</p>
        <p class="text-xl pb-5 text-gray-100">With <span class="font-bold">agentic AI</span> and <span class="font-bold">large language models (LLMs),</span> Netspective’s contract engineering teams empower organizations to achieve unparalleled efficiency, precision, and scalability. From task automation to intelligent decision-making, our solutions are tailored to meet the demands of tomorrow’s workforce, today.</p>
        <!-- <div class="flex justify-center mt-7"><img src={r.uaAssetURL.brand(
          "/www.netspective.com/solutions/contexxio/Contexxio-Your-Patient-and-Clinician-content-in-any-EHR_Real-Time.png#center"
        )} alt=""></div>        -->
        
      </article>
    </section>    
    <section class="mx-auto max-w-7xl px-4 py-4 sm:px-6 lg:px-8">
      <article class="pb-6 curated-content">
        <div class="text-center">
         <h3 class="text-2xl font-bold mb-10 mt-4 text-center inline-flex"><span class="bg-orange-600 text-white py-2 px-8">Netspective AI Workforce Solutions</span></h3>
        </div>
         <Content/>
      <div class="clear"></div>
      </article>
    </section>
  </main>
 
</NetspectiveLayout>
