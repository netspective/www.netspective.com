---
import NetspectiveLayout from "../../../layouts/ds/tailwind/NetspectiveLayout.astro";
import { routes, Route } from "../../../lib/native/route";
import ContactForm  from "../../../components/native/www.netspective.com/ContactForm";
export interface Props {
	readonly route: Route<any, any>;
}


const { route } = Astro.props;
const r = routes(Astro, Astro.props.route);

---

<NetspectiveLayout route={route} {r}>
  <main>
    <section class="mx-auto max-w-7xl px-4 py-4 sm:px-6 lg:px-8">
      <article class="pb-6 curated-content">
        <div class="text-center">
         <h3 class="text-2xl font-bold mb-10 mt-4 text-center inline-flex"><span class="bg-orange-600 text-white py-2 px-8">Contact us </span></h3>
        </div>
        <ContactForm  client:only="react"/>
      </article>
      <article>
        <div class="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-2">
            <div class="mr-3">
              <h3 class="mt-7 mb-3 text-xl font-semibold">Our Office</h3>
                <div class="mb-4 ml-3">
                  <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 shrink-0 text-gray-900 float-left mr-2" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z" />
                    <path stroke-linecap="round" stroke-linejoin="round" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z" />
                  </svg>Netspective Communications LLC, 1802 Brightseat Rd. Suite 101, Landover, MD 20785
                </div>
                <div class="mb-4 ml-3">
                  <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 shrink-0 text-gray-900 float-left mr-2" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
                      <path stroke-linecap="round" stroke-linejoin="round" d="M3 8l7.89 5.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z"/>
                  </svg>Email: sales@netspective.com
                </div>
                <div class="mb-4 ml-3">
                  <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 shrink-0 text-gray-900 float-left mr-2" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z"/>
                  </svg>Phone: (202) 660-1351
                </div>
            </div>
            <div class="ml-3 mb-4">
              <div class="google-map">
                <h3 class="mt-7 mb-3 text-xl font-semibold">We Also Count in Google Maps</h3>
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3104.3961374628702!2d-76.85640518464909!3d38.91492227956801!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89b7c07223701747%3A0x9b325ba0afeec12a!2sNetspective+Media!5e0!3m2!1sen!2sin!4v1530794382166" width="555" height="300" style="border:0" allowfullscreen></iframe>
              </div>
            </div>
          </div>
      </article>
    </section>
  </main>
  <section class="px-8 sm:px-6 lg:px-8 bg-cover bg-no-repeat bg-center py-10 bg-image">
    <article class="mx-auto max-w-6xl py-9 text-white text-center">
      <h3 class="text-2xl font-semibold pb-5">Netspective is a leader in Technology and Consulting services in regulated markets - Healthcare, Government and Medical Technology.</h3>
      <p>Technology, consulting, and solutions focused on firms impacted by FDA, ONC, NIST or other safety, privacy, and security regulations.</p>
    </article>
  </section>
</NetspectiveLayout>



